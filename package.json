{
  "name": "mf-host-template",
  "version": "1.0.0",
  "description": "Webpack's Module Federation Template Configuration for a host app",
  "main": "index.js",
  "scripts": {
    "prod": "concurrently \"npm run routes\" \"npm run prod:css\" concurrently \"npm run prodmode \"",
    "dev": "concurrently \"npm run dev:css\" \"npm run devmode \"",
    "devmode": "webpack serve --mode development",
    "prodmode": "webpack build --mode production --config ./webpack.config.prod.js",
    "dev:css": "tailwindcss -i ./src/assets/tailwind.css -o ./src/assets/main.css --watch",
    "prod:css": "tailwindcss -i ./src/assets/tailwind.css -o ./src/assets/main.css"
  },
